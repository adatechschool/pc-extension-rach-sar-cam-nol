{
    "name": "Empowerment",
    "description": "Une extension feel good pour la vie de tous les jours",
    "version": "1.0.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}